<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
    <system.serviceModel>
      <services>
        <service name="BankService.DataServiceManager">
          <endpoint address="net.tcp://localhost:8010/BankDataService" binding="netTcpBinding" contract="Service.Common.IDataService" />
        </service>
        <service name="BankService.ReportServiceManager">
          <endpoint address="http://localhost:8011/BankReportService" binding="basicHttpBinding" contract="Service.Common.IReportService" />
        </service>
      </services>
    </system.serviceModel>
</configuration>